{
  "name": "example",
  "private": true,
  "version": "1.20.0",
  "license": "Apache 2.0",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "build:clean": "npm run clean && gatsby build",
    "build:prefix": "gatsby build --prefix-paths",
    "dev": "gatsby develop -H 0.0.0.0",
    "dev:clean": "gatsby clean && gatsby develop -H 0.0.0.0",
    "clean": "gatsby clean",
    "deploy": "gh-pages -d public",
    "predeploy": "npm run build",
    "serve": "gatsby serve",
    "format": "prettier --write \"**/*.{css,scss,json,html,yaml}\"",
    "serve:prefix": "gatsby serve --prefix-paths",
    "prepare": "husky install"
  },
  "dependencies": {
    "@carbon/icons-react": "^10.13.0",
    "gatsby": "^5.8.1",
    "gatsby-plugin-manifest": "^5.8.0",
    "gatsby-theme-carbon": "^3.1.0",
    "ibm-video-streaming-web-player-api": "^1.2.0",
    "md5": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1 || ^17.0.2",
    "react-dom": "^16.13.1 || ^17.0.2",
    "react-helmet": "^6.0.0",
    "smooth-scroll": "^16.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.4.1",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-wesbos": "3.0.2",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.21.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gatsby-cli": "2.12.12",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.0.2",
    "stylelint": "^14.11.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-scss": "^4.3.0"
  },
  "eslintIgnore": [
    "**/node_modules/",
    "**/public/",
    "**/.cache/",
    "**/build/"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    "*.{css,scss,json,html,yaml}": [
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ]
  }
}
